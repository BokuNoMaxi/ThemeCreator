{
    "name": "bokuno/theme-creator",
    "type": "typo3-cms-extension",
    "description": "Theme creator for yet_another_bootstrap_template",
    "license": "GPL-2.0-or-later",
    "require": {
      "typo3/cms-core": "^12.4"
    },
    
    "autoload": {
      "psr-4": {
        "BokuNo\\ThemeCreator\\": "Classes"
      }
    },
    "config": {
      "vendor-dir": ".Build/vendor",
      "bin-dir": ".Build/bin"
    },
    "extra": {
      "typo3/cms": {
        "web-dir": ".Build/public",
        "extension-key": "theme_creator"
      }
    }
  }
  